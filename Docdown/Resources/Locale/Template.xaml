    <System:String x:Key="Workspace_Open_Failed">Could not open workspace</System:String>

    <System:String x:Key="Workspace_Explorer_Open">Open</System:String>
    <System:String x:Key="Workspace_Explorer_Add">Add</System:String>
    <System:String x:Key="Workspace_Explorer_Add_Existing">Existing File</System:String>
    <System:String x:Key="Workspace_Explorer_Add_New">New File</System:String>
    <System:String x:Key="Workspace_Explorer_Add_Folder">New Folder</System:String>
    <System:String x:Key="Workspace_Explorer_Open_Windows">Open in explorer</System:String>
    <System:String x:Key="Workspace_Explorer_Rename">Rename</System:String>
    <System:String x:Key="Workspace_Explorer_Delete">Delete</System:String>
    <System:String x:Key="Workspace_Explorer_Exclude">Exclude</System:String>
